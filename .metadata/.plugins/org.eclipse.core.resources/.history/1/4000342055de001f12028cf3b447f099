package com.foodie.foodiebackend;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.ComponentScan;

@SpringBootApplication
@ComponentScan(basePackages = {"com.foodie.foodiebackend.controller", "com.foodie.foodiebackend.repository"})
public class FoodiebackendApplication {

	public static void main(String[] args) {
		SpringApplication.run(FoodiebackendApplication.class, args);
	}

}
